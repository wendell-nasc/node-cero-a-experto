teste
::EXIBIR::::

git remote show--->exibir branches



::ADICIONAR REMOTO:::
git remote add origin git@github.com:wendell-nasc/node-cero-a-experto.git


:::ENVIAR REMOTO::::

git add .
git status
git log
git branch

git branch aula1
git branch aula2
git branch aula3
git branch aula4


git checkout aula1
git checkout aula2
git checkout aula3
git checkout aula4


git commit . -m "aula1"
git commit . -m "aula2"
git commit . -m "aula3"
git commit . -m "aula4"


git remote show--->exibir branches


git remote add aula1 git@github.com:wendell-nasc/node-cero-a-experto.git
git remote add aula2 git@github.com:wendell-nasc/node-cero-a-experto.git
git remote add aula-03 git@github.com:wendell-nasc/node-cero-a-experto.git
git remote add aula-04 git@github.com:wendell-nasc/node-cero-a-experto.git

-->ENVIAR
git push -u aula3 aula-03
git push -u aula-04 aula4

-->PUXAR


